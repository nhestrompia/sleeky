{
  "manifest_version": 3,
  "name": "Sleeky",
  "description": "Enhance and refine your web design with AI-powered tools and insights",
  "version": "1.0.0",

  "action": {
    "default_title": "Click",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "tabs", "scripting"],

  "options_page": "options.html"
}
